{
  "expo": {
    "name": "EASI by Epico",
    "slug": "easi-by-epico",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "easiapp",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.epico.easi",
      "config": {
        "googleMapsApiKey": "AIzaSyCNpWjIoH986AQx2ea2AaiqzsqUcwaqX9I"
      },
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow EASI to use Face ID for secure and convenient login authentication."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.epico.easi",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCNpWjIoH986AQx2ea2AaiqzsqUcwaqX9I"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-location"
    ]
  }
}
